@model IEnumerable<string>

@Html.ActionLink("All", "List", "Product", null, new { @class = "btn btn-default" })

@foreach (var prodtype in Model) {
    @Html.RouteLink(prodtype, new { controller = "Product",
    action = "List",
    category = prodtype,
    page = 1 }, 
    new { @class = "btn btn-default" })
}